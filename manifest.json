{
  "background_page": "background.html",
  "description": "Official Storify.com Chrome Extension - make social media stories",
  "icons": {
    "16":  "storify-this.ico",
    "32":  "storify-icon-32.png",
    "48":  "storify-icon-48.png",
    "128": "storify-icon.png"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["storify.js"]
  }],
 "minimum_chrome_version": "5.0",
 "name": "Storify Extension",
 "permissions": [ "contextMenus", "tabs", "*://*.storify.com/" ],
 "version": "0.2.6"
}
